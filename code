#include <iostream>

using namespace std;

int main()
{
    int a, b, c, sa, sb, sc, smax = 0;
    
    cin >> a >> b >> c;
    
    sa = a % 10 + a / 10 % 10;
    sb = b % 10 + b / 10 % 10;
    sc = c % 10 + c / 10 % 10;
    
    if(smax < sa)
    {
        smax = sa;
    }
    
    if(smax < sb)
    {
        smax = sb;
    }
    
    if(smax < sc)
    {
        smax = sc;
    }

    if(smax == sa)
    {
        cout << a << ' ';
    }

    if(smax == sb)
    {
        cout << b << ' ';
    }
    
    
    if(smax == sc)
    {
        cout << c << ' ';
    }
    
    return 0;
}
